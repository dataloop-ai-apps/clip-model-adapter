      "projectId": null,
      "config": {
        "package": {
          "code": "import dtlpy as dl\n\nclass ServiceRunner:\n    @staticmethod\n    def run(item: dl.Item, model: dl.Model):\n        filters = dl.Filters(resource=dl.FiltersResource.FEATURE_SET)\n        filters.add(field=\"modelId\", values=model.id)\n        feature_sets_pages = item.project.feature_sets.list(filters=filters)\n\n        if feature_sets_pages.items_count == 0:\n            raise ValueError(f\"No Features Sets were found for Model: '{model.id}'\")\n        elif feature_sets_pages.items_count > 1:\n            raise ValueError(f\"Multiple Features Sets were found for Model: '{model.id}'\")\n        feature_set: dl.FeatureSet = list(feature_sets_pages.all())[0]\n\n        filters = dl.Filters(resource=dl.FiltersResource.FEATURE)\n        filters.add(field=\"featureSetId\", values=feature_set.id)\n        filters.add(field=\"entityId\", values=item.id)\n        features_pages = feature_set.features.list(filters=filters)\n\n        if features_pages.items_count == 0:\n            raise ValueError(f\"None of the Features in the Feature Sets was associated with the Item: '{item.id}'\")\n        elif features_pages.items_count > 1:\n            raise ValueError(f\"Multiple Features from the same model are associated with the Item: '{item.id}'\")\n        feature: dl.Feature = list(features_pages.all())[0]\n\n        if len(feature.value) == 0:\n            raise ValueError(f\"Feature Value is empty for Feature: '{feature.id}'\")\n",
          "name": "run",
          "type": "code",
          "codebase": {
            "type": "item"
          }
        }
